{
  "apps": [
    {
      "name": "blog-serve",
      "cwd": "./",
      "script": "app/server.js",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "error_file": "log/node-app.stderr.log",
      "out_file": "log/node-app.stdout.log",
      "instances": 6,
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "500M",
      "watch": true,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "fork",
      "autorestart": true,
      "vizion": false
    }
  ]
}